#include<stdio.h>

using namespace std;

int main() {
	int num;
  char result[51], input[51];
	scanf("%d%s", &num, result);

	for (int i = 0; i < num - 1; i++) {
		scanf("%s", input);
		for (int j = 0; result[j] != '\0'; j++) {
			if (result[j]=='?') {
				continue;
			}
			else {
				if (result[j] == input[j]) {
					continue;
				}
				else {
					result[j] = '?';
				}
			}
		}
	}
	printf("%s\n", result);
}
