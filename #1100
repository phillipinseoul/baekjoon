#include <stdio.h>

int main() {
	char chess[9][9];
	int i, j, cnt = 0;
	for (i = 0; i < 8; i++) {
		scanf("%s", chess[i]);
	}
	
	int k = 0;
	for (i = 0; i < 8; i++) {
		for (j = 0; j < 4; j++) {
			if (chess[i][2 * j + k] == 'F') cnt++;
		}
		k = 1 - k;
	}
	printf("%d\n", cnt);
}

// code length = 281 B
