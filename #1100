#include <stdio.h>

int main() {
	char chess[9][9];
	int i, j, cnt = 0;
	for (i = 0; i < 8; i++) {
		scanf("%s", chess[i]);
	}
	for (i = 0; i < 8; i++) {
		if (i % 2 == 0) { // 첫칸 = 흰색
			for (j = 0; j < 4; j++) {
				if (chess[i][2 * j] == 'F') cnt++;
			}
		}
		else { // 첫칸 = 검은색
			for (j = 0; j < 4; j++) {
				if (chess[i][2 * j + 1] == 'F') cnt++;
			}
		}
	}
	printf("%d\n", cnt);	
	return 0;
}

// code length = 420 B
