#include<cstdio>
// <iostream>을 <cstdio>로 대체함
// map 사용할 필요 없다!

using namespace std;
char board[500];

// 백준 1343번

int main() {
	// take input
	scanf("%s", &board);
	
	// map 제거 -> string 교체 바로바로 실행
	int cnt = 0;
	for (int i = 0; board[i] != '\0'; i++) {
		if (board[i] == 'X') {
			cnt++;
			if (board[i + 1] == '.' || board[i+1] == '\0') {
				if (cnt % 2 != 0){
                    			printf("-1");
                   			return 0;
               	 		}
				else if (cnt % 4 == 0){
					for (int j = i + 1 - cnt; j < i + 1; j++) board[j] = 'A';
				}
				else {
					for (int j = i + 1 - cnt; j < i + 1; j++) board[j] = 'A';
					board[i - 1] = 'B';
					board[i] = 'B';
				}
				cnt = 0;
			}
		}
	}
	printf("%s\n", board);
	return 0;
}

// 메모리 = 1112KB -> 메모리 사용량 감소함
// #include<iostream> 사용하기 때문! -> #include<cstdio>로 대체하면 메모리 사용량 감소함
