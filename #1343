#include<iostream>
#include<map>

using namespace std;

// 백준 1343번

int main() {
	// take input
	char board[500];
	scanf("%s", &board);

	// make a map<int, int> -> <starting index, length>
	map<int, int> xs;
	int cnt = 0, addr = 0;

	for (int i = 0; board[i] != '\0'; i++) {
		if (board[i] == 'X') {
			if (board[i + 1] == '.' || board[i+1] == '\0') {
				cnt++;
				if(cnt % 2 != 0){
                    			printf("-1");
                   			return 0;
               	 		}
				xs[i + 1 - cnt] = cnt;
				cnt = 0;
				continue;
			}
			cnt++;
		}
	}

	// ex) XX.XX -> xs = {0: 2, 3: 2}

	// 6 = 4 + 2, 8 = 4 + 4
	// check each fragment
	for (auto it = xs.begin(); it != xs.end(); it++) {
		int start = it->first, cons = it->second;

		if (cons % 4 == 0) {
			for (int i =start; i < start + cons; i++) {
				board[i] = 'A';
			}
		}
		else {
			for (int i = start; i < start + cons - 2; i++) {
				board[i] = 'A';
			}
			board[start + cons - 2] = 'B';
			board[start + cons - 1] = 'B';
		}
	}

	printf("%s\n", board);
	return 0;
}

// 메모리 = 1988KB -> 메모리 어디서 많이 사용?
