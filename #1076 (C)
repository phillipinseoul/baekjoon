#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <string.h>   // strcmp
// get rid of <math.h> => pow 한번밖에 사용안함! for-loop으로 대체 가능함

// instead of using functions, use for-loop 3 times in the main()

int main() {
	int i;
	long long result, mul = 1;
	char c1[10], c2[10], c3[10];
	scanf("%s\n%s\n%s", c1, c2, c3);		// take all inputs AT ONCE!
	
	for (i = 0; i < 10; i++) {
		if (strcmp(c1, color[i]) == 0) result = 10 * i;
	}
	for (i = 0; i < 10; i++) {
		if (strcmp(c2, color[i]) == 0) result += i;
	}
	for (i = 0; i < 10; i++) {
		if (strcmp(c3, color[i]) == 0) result *= mul;
		mul *= 10;
	}
	printf("%lld\n", result);
}
